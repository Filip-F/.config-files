	name = Denton Liu
[push]
	default = matching
[alias]
	git = !git
	no-ff-merge = merge --no-ff
	graph = log --graph
	supush = push --set-upstream
	prune-merged = !sh -c 'git branch -d $(git branch --merged HEAD | grep -v "^*")'
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[merge]
	conflictStyle = diff3
	tool = vimdiff
[mergetool]
	prompt = false
[commit]
	cleanup = scissors
[core]
	excludesfile = ~/.globalgitignore
[help]
	autocorrect = 10
